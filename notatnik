        <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 53.064820, lng: 21.564880},
          zoom: 8
        });
      }
    </script>    
    <script async defer src="https://naps.googleapis.com/maps/api/js?key=AIzaSyCFiWJZPMBDzIdnroJwpb0lyWk8xy_HTZk&callback=initMap"></script>
</div>
